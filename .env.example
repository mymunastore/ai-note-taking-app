# Clerk Authentication (Required for production)
# Get your keys from https://dashboard.clerk.com/last-active?path=api-keys
VITE_CLERK_PUBLISHABLE_KEY=

# Stripe Billing (Required for billing features)
# Get your keys from https://dashboard.stripe.com/apikeys
VITE_STRIPE_PUBLISHABLE_KEY=

# PostHog Analytics (Required for analytics)
# Get your keys from https://app.posthog.com/project/settings
VITE_POSTHOG_KEY=
VITE_POSTHOG_HOST=https://app.posthog.com

# Supabase Database (Required for enhanced data storage)
# Get your keys from https://app.supabase.com/project/_/settings/api
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# Application Configuration
VITE_APP_URL=http://localhost:5173
VITE_API_URL=http://localhost:4000

# Feature Flags
VITE_ENABLE_WORKFLOWS=false
VITE_ENABLE_COHERE=false

# Demo Mode (automatically enabled if Clerk key is missing or invalid)
VITE_DEMO_MODE=false

# Backend Secrets (Set these in Encore Cloud or local development)
# Get Clerk secret key from https://dashboard.clerk.com/last-active?path=api-keys
# ClerkSecretKey=

# Get Stripe secret key from https://dashboard.stripe.com/apikeys
# StripeSecretKey=

# Get Stripe webhook secret from https://dashboard.stripe.com/webhooks
# StripeWebhookSecret=

# Get OpenAI API key from https://platform.openai.com/api-keys
# OpenAIKey=

# Get Resend API key from https://resend.com/api-keys
# ResendApiKey=

# Get Supabase service role key from https://app.supabase.com/project/_/settings/api
# SupabaseUrl=
# SupabaseServiceKey=

# Optional: Get Cohere API key from https://dashboard.cohere.ai/api-keys
# CohereApiKey=

# Optional: Get Make.com API key from https://www.make.com/en/help/tools/api
# MakeApiKey=
# MakeTeamId=
