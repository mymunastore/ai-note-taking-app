# Clerk Authentication (Required for production)
VITE_CLERK_PUBLISHABLE_KEY=pk_test_your_clerk_publishable_key_here

# Stripe Billing (Required for billing features)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# Application Configuration
VITE_APP_URL=http://localhost:5173
VITE_API_URL=http://localhost:4000

# Feature Flags
VITE_ENABLE_WORKFLOWS=false
VITE_ENABLE_COHERE=false

# Demo Mode (automatically enabled if Clerk key is missing)
VITE_DEMO_MODE=false

# Backend Secrets (Set these in Encore Cloud or local development)
# ClerkSecretKey=sk_test_your_clerk_secret_key_here
# StripeSecretKey=sk_test_your_stripe_secret_key_here
# StripeWebhookSecret=whsec_your_stripe_webhook_secret_here
# OpenAIKey=sk-your_openai_api_key_here
# CohereApiKey=your_cohere_api_key_here
# MakeApiKey=your_make_api_key_here
# MakeTeamId=your_make_team_id_here
